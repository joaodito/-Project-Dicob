package model;

public class Streaming extends Expense{
	
	private String platform;
	private double annualValue;
	
	public Streaming(int idExpense,String nameExpense,int dateExpense,String platform, double annualValue){
		super(idExpense, nameExpense, dateExpense);
		this.platform = platform;
		this.annualValue = annualValue;
	}
	
	public Streaming(){}
	
	public int getIdExpense() {
		return super.getIdExpense();
	}
	
	public String getNameExpense() {
		return super.getNameExpense();
	}
	
	public int getDateExpense() {
		return super.getDateExpense();
	}

	public String getPlatform() {
		return platform;
	}	
	
	public double getAnnualValue() {
		return annualValue;
	}
	
	public void setDateExpense(int dateExpense) {
		super.setDateExpense(dateExpense);
	}
	
	public void setNameExpense(String nameExpense) {
		super.setNameExpense(nameExpense);
	}
	
	public void setIdExpense(int idExpense) {
		super.setIdExpense(idExpense);
	}
	
	public void setPlatform(String platform) {
		this.platform = platform;
	}
	
	public void setAnnualValue(double annualValue) {
		this.annualValue = annualValue;
	}
	
	public String toString() {
		return getIdExpense()+" "+getNameExpense()+""+getDateExpense()+platform+" "+annualValue;
	}
}
